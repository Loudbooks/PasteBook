		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"sass-embedded": "^1.89.1",
		"svelte": "^5.33.14",
		"svelte-check": "^4.2.1",
		"typescript": "^5.8.3",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"shiki": "^3.4.2"
